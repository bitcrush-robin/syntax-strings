{
    "name": "syntax-strings",
    "displayName": "syntax-strings",
    "description": "Highlight strings in python as sql, html, glsl, or more python",
    "version": "1.0.0",
    "publisher": "bitcrush",
    "repository": {
        "url": "https://github.com/bitcrush-robin/syntax-strings"
    },
    "engines": {
        "vscode": "^1.32.0"
    },
    "categories": [
        "Programming Languages",
        "Snippets"
    ],
    "contributes": {
        "grammars": [
            {
                "path": "./syntaxes/python-highlight-sql-string.json",
                "scopeName": "python-sql.injection",
                "injectTo": [
                    "source.python"
                ],
                "embeddedLanguages": {
                    "meta.embedded.sql": "sql"
                }
            },
            {
                "path": "./syntaxes/python-highlight-html-string.json",
                "scopeName": "python-html.injection",
                "injectTo": [
                    "source.python"
                ],
                "embeddedLanguages": {
                    "meta.embedded.block.html": "html"
                }
            },
            {
                "path": "./syntaxes/python-highlight-python-string.json",
                "scopeName": "python-python.injection",
                "injectTo": [
                    "source.python"
                ],
                "embeddedLanguages": {
                    "meta.embedded.python": "python"
                }
            },
            {
                "path": "./syntaxes/python-highlight-glsl-string.json",
                "scopeName": "python-glsl.injection",
                "injectTo": [
                    "source.python"
                ],
                "embeddedLanguages": {
                    "meta.embedded.glsl": "glsl"
                }
            }
        ]
    }
}
